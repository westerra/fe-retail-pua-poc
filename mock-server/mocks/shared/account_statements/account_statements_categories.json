{
  "name": "/client-api/v2/account/statements/categories",
  "request": {
    "url": "/client-api/v2/account/statements/categories(\\?.*)?$",
    "method": "GET"
  },
  "responses": {
    "badRequest": {
      "status": 400,
      "data": {
        "message": "Bad Request",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Value Exceeded. Must be between {min} and {max}.",
            "key": "common.api.shoesize",
            "context": {
              "max": "50",
              "min": "1"
            }
          }
        ]
      }
    },
    "forbidden": {
      "status": 403,
      "data": {
        "message": "Access to requested resource denied.",
        "key": "GENERAL_ERROR",
        "errors": [
          {
            "message": "Resource access denied due to an insufficient user quota of {quota}.",
            "key": "common.api.quota",
            "context": {
              "quota": "someQuota"
            }
          }
        ]
      }
    },
    "systemError": {
      "status": 500,
      "data": {
        "message": "Description of error"
      }
    },
    "success": {
      "default": true,
      "status": 200,
      "data": {
        "categories": ["Account", "Credit Card", "Mortgage", "Tax Form", "LOC", "EELOC"],
        "additions": {
          "categoryId": "1"
        }
      }
    }
  }
}
